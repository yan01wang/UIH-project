            <fo:table table-layout="fixed" width="100%" space-before="5pt" space-after="5pt" border-before-style="solid" border-before-width="0.5pt" border-before-color="#B0C6CD" border-after-style="solid" border-after-width="0.5pt" border-after-color="#B0C6CD" border-end-style="solid" border-end-width="0.5pt" border-end-color="#B0C6CD" border-start-style="solid" border-start-width="0.5pt" border-start-color="#B0C6CD">
