                <fo:table-header start-indent="0pt">
                    <fo:table-row>
                        <fo:table-cell display-align="center" text-align="center" background-color="#B0C6CD" border-right="0.5pt solid white" border-after-style="solid" border-after-width="0.5pt" border-after-color="#B0C6CD" border-before-style="solid" border-before-width="0.5pt" border-before-color="#B0C6CD"><fo:block>
                            <fo:block space-before="5pt" space-after="5pt" text-indent="0em" margin-top="5pt" margin-bottom="5pt" line-height="14pt">场地</fo:block>
                        </fo:block></fo:table-cell>
                        <fo:table-cell display-align="center" text-align="center" background-color="#B0C6CD" border-after-style="solid" border-after-width="0.5pt" border-after-color="#B0C6CD" border-before-style="solid" border-before-width="0.5pt" border-before-color="#B0C6CD"><fo:block>
