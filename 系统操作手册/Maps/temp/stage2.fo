dth="60%" content-height="60%" alignment-baseline="middle" src="url('file:/D:/UIM%E9%A1%B9%E7%9B%AE/012%20Data/LianYing/data/General/%E7%B3%BB%E7%BB%9F%E6%93%8D%E4%BD%9C%E6%89%8B%E5%86%8C/images/figure3-8.png')"/></fo:block></fo:block></fo:list-item-body></fo:list-item></fo:list-block>
