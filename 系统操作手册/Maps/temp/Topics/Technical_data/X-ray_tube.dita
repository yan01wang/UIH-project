            <ul class="- topic/ul " id="ul_j4l_x5l_cdb" xtrc="ul:2;30:37" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/Technical_data/X-ray_tube.dita">
