                    <stentry class="- topic/stentry " xtrc="stentry:4;26:30" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Patient_table.dita">释放按钮</stentry>
                </strow>
                <strow class="- topic/strow " xtrc="strow:3;28:24" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Patient_table.dita">
                    <stentry class="- topic/stentry " xtrc="stentry:5;29:30" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Patient_table.dita">垂直升降部分</stentry>
                    <stentry class="- topic/stentry " xtrc="stentry:6;30:30" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Patient_table.dita">床把手</stentry>
                </strow>
                <strow class="- topic/strow " xtrc="strow:4;32:24" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Patient_table.dita">
