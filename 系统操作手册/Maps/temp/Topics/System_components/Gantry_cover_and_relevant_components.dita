            </strow>
            <strow class="- topic/strow " xtrc="strow:2;30:20" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Gantry_cover_and_relevant_components.dita">
