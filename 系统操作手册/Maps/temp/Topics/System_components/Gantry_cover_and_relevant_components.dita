                <stentry class="- topic/stentry " xtrc="stentry:4;32:26" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Gantry_cover_and_relevant_components.dita">呼吸导航</stentry>
            </strow>
            <strow class="- topic/strow " xtrc="strow:3;34:20" xtrf="file:/D:/UIM项目/012%20Data/LianYing/data/General/系统操作手册/Topics/System_components/Gantry_cover_and_relevant_components.dita">
